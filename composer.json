{
  "name": "adelowo/naweown",
  "description": "POC",
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.0",
    "laravel/framework": "5.3.*"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~5.0",
    "symfony/css-selector": "3.1.*",
    "symfony/dom-crawler": "3.1.*",
    "doctrine/dbal": "^2.5"
  },
  "authors": [
    {
      "name": "Lanre Adelowo",
      "homepage": "http://lanreadelowo.com",
      "email": "me@lanreadelowo.com",
      "role": "Developer"
    }
  ],
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "Naweown\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ],
    "psr-4": {
      "Tests\\": "tests/"
    },
    "files": [
      "app/functions.php"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan optimize"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
